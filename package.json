{
  "name": "@bowtie/utils",
  "version": "0.1.9",
  "description": "Helpful JS utilities",
  "main": "index.js",
  "engines": {
    "node": ">=6.x"
  },
  "scripts": {
    "doc": "jsdoc --configure .jsdoc.json --verbose",
    "doc:commit": "npm run doc && git add docs && git commit -m 'Generated Docs'",
    "build": "babel src -d lib --presets env",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run lint && npm run build && mocha test/**/*.spec.js",
    "test:watch": "mocha test/**/*.spec.js --watch",
    "preversion": "npm test",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "bowtie",
    "utils",
    "utilities",
    "help",
    "helpers"
  ],
  "author": "Bowtie",
  "license": "MIT",
  "standard": {
    "ignore": [
      "docs/*"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "mocha": "^5.0.0",
    "standard": "^10.0.3"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bowtie-co/node-utils.git"
  },
  "bugs": {
    "url": "https://github.com/bowtie-co/node-utils/issues"
  },
  "homepage": "https://github.com/bowtie-co/node-utils#readme"
}
